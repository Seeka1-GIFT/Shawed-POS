{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html",
      "methods": ["GET"]
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "VITE_API_URL": "https://shawed-pos.onrender.com/api"
  }
}


